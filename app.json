{
  "expo": {
    "name": "GOgps",
    "slug": "GOgps",
    "version": "1.0.1",
    "orientation": "portrait",
    "privacy": "public",
    "platforms":["ios","android"],
    "android": {
      "package": "com.padgad.gogpstest"
    },
    "icon": "./assets/gps.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    } 
    
  }
}
